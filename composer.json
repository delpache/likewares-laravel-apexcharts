{
    "name": "likewares/laravel-apexcharts",
    "description": "Package ApexCharts pour Laravel",
    "version": "1.0.0",
    "keywords": [
        "laravel",
        "line",
        "bar",
        "pie",
        "donut",
        "charts",
        "apexcharts"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "KOUAOBILE ASSANDE HUGUES",
            "email": "assandehugues@gmail.com",
            "homepage": "https://likewares.com",
            "role": "CEO Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3|^8.4",
        "ext-json": "*",
        "balping/json-raw-encoder": "^1.0|dev-master",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5|^1.6|^1.7",
        "phpunit/phpunit": "^9.5|^10.0|^11.0|^12.0",
        "orchestra/testbench": "^7.4|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Likewares\\Apexcharts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Likewares\\Apexcharts\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Likewares\\Apexcharts\\Provider"
            ],
            "aliases": {
                "Apexcharts": "Likewares\\Apexcharts\\Facade"
            }
        }
    }
}
